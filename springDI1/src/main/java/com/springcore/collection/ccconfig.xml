<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean name = "emp" class="com.springcore.collection.Employee">
	
	<property name="name" value="vishal" />
	
	<property name="phones">
	<list><value>10145555</value>
	<value>1022321</value>
	<value>103456</value>
	</list>
	</property>
	
	<property name="addresses">
	<set><value>delhi</value>
	<value>mumbai</value>
	<value>goa</value>
	</set>
	</property>
	
	<property name="courses">
	<map>
	<entry key="java" value="2month"/>
	<entry key="python" value="3month"/>
	<entry key="dotnet" value="4month"/>
	</map>
	</property>
	
	
	
	
	
	
	</bean>
	

	<context:annotation-config/>  
</beans>